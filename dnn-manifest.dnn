<dotnetnuke type="Package" version="9.0">
  <packages>
    <package name="nvisionative.nvquicktheme" type="Skin" version="0.9.0">
      <friendlyName>nvQuickTheme</friendlyName>
      <description>A Bootstrap 4 DNN Framework</description>
      <iconFile>MyIcon.png</iconFile>
      <owner>
        <name>nvisionative</name>
        <organization>nvisionative</organization>
        <url>www.nvisionative.com</url>
        <email>support@nvisionative.com</email>
      </owner>
      <license src="LICENSE" />
      <dependencies>
        <dependency type="coreVersion">09.00.00</dependency>
      </dependencies>
      <components>
        <component type="Skin">
          <skinFiles>
            <basePath>Portals\_default\Skins\nvQuickTheme</basePath>
            <skinName>nvQuickTheme</skinName>
            
            <!-- Main Directory -->
            <skinFile>
              <path />
              <name>default.ascx</name>
            </skinFile>
            <skinFile>
              <path />
              <name>default.doctype.xml</name>
            </skinFile>
            <skinFile>
              <path />
              <name>default.png</name>
            </skinFile>
            <skinFile>
              <path />
              <name>thumbnail_default.png</name>
            </skinFile>
            <skinFile>
              <path />
              <name>LICENSE</name>
            </skinFile>
                        
          </skinFiles>
        </component>
        
        <!-- Partials Folder -->
        <component type="ResourceFile">
          <resourceFiles>
            <basePath>Portals\_default\Skins\nvQuickTheme\partials\</basePath>
            <resourceFile>
                <name>partials/partials.zip</name>
            </resourceFile>
          </resourceFiles>
        </component>
        
        <!-- Menus Folder -->
        <component type="ResourceFile">
          <resourceFiles>
            <basePath>Portals\_default\Skins\nvQuickTheme\menus\main\</basePath>
            <resourceFile>
                <name>menus/main/menus.zip</name>
            </resourceFile>
          </resourceFiles>
        </component>
        
        <!-- CSS Folder -->
        <component type="ResourceFile">
          <resourceFiles>
            <basePath>Portals\_default\Skins\nvQuickTheme\dist\css\</basePath>
            <resourceFile>
                <name>dist/css/css.zip</name>
            </resourceFile>
          </resourceFiles>
        </component>
        
        <!-- JS Folder -->
        <component type="ResourceFile">
          <resourceFiles>
            <basePath>Portals\_default\Skins\nvQuickTheme\dist\js\</basePath>
            <resourceFile>
                <name>dist/js/js.zip</name>
            </resourceFile>
          </resourceFiles>
        </component>
        
        <!-- Fonts Folder -->
        <component type="ResourceFile">
          <resourceFiles>
            <basePath>Portals\_default\Skins\nvQuickTheme\dist\fonts\</basePath>
            <resourceFile>
                <name>dist/fonts/fonts.zip</name>
            </resourceFile>
          </resourceFiles>
        </component>
        
      </components>
    </package>
  </packages>
</dotnetnuke>